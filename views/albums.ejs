<h1>Albums</h1>

<% artistAlbums.forEach((album) =>{%>

    <p style="font-weight:bold"><%- album.name %></p>
    
    <% if(album.images.length > 0) {%>
        <a href="<%- album.href %>">
        <img src="<%- album.images[0].url %>" width="100"/></a>
        <%}%>
    <br/>
    <a href="/tracks/<%- album.id %>">View Tracks</a>
    <br/><br/>


    <%}) %>